config_version=5

[application]

config/name="Dwelt"
config/description="An environment-driven puzzle RPG about robot cartels and essences of emotion."
config/version="1.0.1"
run/main_scene="res://maps/tech_test_note/tech_test_note.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="dwelt"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://generic/tex/splash.png"
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio_bus.tres"

[autoload]

Global="*res://lib/global.gd"
Utilities="*res://lib/utilities.gd"
Action="*res://lib/action.gd"
Save="*res://lib/save.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/initial_position=Vector2i(50, 50)
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"
mouse_cursor/custom_image="res://lib/ui/tex/cursor.png"

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

